<idea-plugin>
  <id>com.pure.share</id>
  <name>android shared preferences bean getter setter</name>
  <version>1.0</version>
  <vendor email="huangxingzhan@gmain.com" url="https://github.com/purecuke">Pure Cuke</vendor>

  <description><![CDATA[
      Share Preferences Getter and Setter is designed to generate getters and setters for Android projects<br>
      support int,long,boolean,float,String<br>
      For more details, look at the repo on GitHub:<br>
      <a href="https://github.com/purecuke/AndroidShareBean">https://github.com/purecuke/AndroidShareBean</a>
      ]]></description>

  <change-notes><![CDATA[
        Version 1.0
        <ul>
            <li>Initial release</li>
        </ul>
        ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="145.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <action class="com.pure.share.ShareAction"
            id="com.pure.share.ShareAction"
            text="Share Preferences Getter and Setter"
            description="Generates Android Share Preferences">
      <add-to-group group-id="GenerateGroup" anchor="last"/>
    </action>
    <!-- Add your actions here -->
  </actions>

</idea-plugin>